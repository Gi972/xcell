{
  "extends": "../../tsconfig.common.json",
  "compilerOptions": {
    "outDir": "dist",
    "removeComments": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "target": "es2015",
    "types": [
      "node"
    ],
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/**/*.test.ts"
  ]
}
